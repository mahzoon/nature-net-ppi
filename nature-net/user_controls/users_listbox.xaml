<UserControl x:Class="nature_net.user_controls.users_listbox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:uc="clr-namespace:nature_net.user_controls"
             mc:Ignorable="d" 
             d:DesignWidth="300">

    <DockPanel LastChildFill="True">
        <Border DockPanel.Dock="Top" BorderThickness="1" BorderBrush="Gray">
            <uc:list_header x:Name="header"></uc:list_header>
        </Border>
        <StackPanel Name="signup_panel" DockPanel.Dock="Top" Margin="0"></StackPanel>
        <uc:custom_listbox_v2 x:Name="users_list" />
    </DockPanel>
    
</UserControl>
